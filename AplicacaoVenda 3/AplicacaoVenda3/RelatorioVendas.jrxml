	<queryString language="SQL">
		<![CDATA[SELECT vendas.venda.cliente,
	vendas.venda.datavenda,
	vendas.itemvenda.quantidade,
	vendas.produto.descricao,
	vendas.produto.valor
FROM vendas.itemvenda
	INNER JOIN vendas.venda ON 
	 vendas.itemvenda.venda = vendas.venda.idvenda 
	INNER JOIN vendas.produto ON 
	 vendas.itemvenda.produto = vendas.produto.idproduto 
WHERE 
	 idvenda = 2]]>
